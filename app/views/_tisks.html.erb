<ul class="media-list">
  
<% tiskss.each do |tisk| %>

    
<% user = tisk.user %>

    <li class="media">
      <div class="media-left">
        <img class="media-object img-rounded" src="
<%= gravatar_url(current_user, options = { size: 50 }) %>
" alt="">
      </div>
      <div class="media-body">
        <div>
          
<%= link_to user.name, user_path(user) %>
 <span class="text-muted">posted at 
<%= tisk.created_at %>
</span>
        </div>
        <div>
          <p>
<%= tisk.content %>
</p>
        </div>
      </div>
    </li>
  
<% end %>

  
<%= paginate tisks %>

</ul>
